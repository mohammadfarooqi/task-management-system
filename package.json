{
  "name": "@task-management-system/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "// Development - Primary Commands //": "",
    "start": "npx nx run-many --target=serve --projects=api,dashboard --parallel",
    "start:api": "npx nx serve api",
    "start:dashboard": "npx nx serve dashboard",
    "dev": "npx nx run-many --target=serve --projects=api,dashboard --parallel",
    "dev:api": "npx nx serve api",
    "dev:dashboard": "npx nx serve dashboard",
    "// Building //": "",
    "build": "npx nx run-many --target=build --all",
    "build:api": "npx nx build api",
    "build:dashboard": "npx nx build dashboard",
    "build:prod": "npx nx run-many --target=build --all --configuration=production",
    "// Testing //": "",
    "test": "npx nx run-many --target=test --all",
    "test:api": "npx nx test api",
    "test:dashboard": "npx nx test dashboard",
    "test:auth": "npx nx test auth",
    "test:coverage": "npx nx run-many --target=test --all --coverage",
    "test:affected": "npx nx affected --target=test",
    "// Code Quality //": "",
    "lint": "npx nx run-many --target=lint --all",
    "lint:api": "npx nx lint api",
    "lint:dashboard": "npx nx lint dashboard",
    "lint:fix": "npx nx run-many --target=lint --all --fix",
    "format": "npx nx format:write",
    "format:check": "npx nx format:check",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "// Utilities //": "",
    "graph": "npx nx graph",
    "reset": "npx nx reset",
    "// Maintenance //": "",
    "clean": "rm -rf dist .angular node_modules/.cache tmp coverage",
    "clean:full": "rm -rf dist .angular node_modules pnpm-lock.yaml tmp coverage",
    "install:clean": "npm run clean:full && pnpm install",
    "db:reset": "rm -f data/database.sqlite && npm run start:api"
  },
  "private": true,
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
